{
  "name": "svg-2-jsx",
  "productName": "SVG to JSX",
  "version": "0.0.1",
  "description": "Get in SVG get out react",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/saravieira/svg-2-jsx.git"
  },
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "mac": "electron-packager ./ --out=./builds --overwrite --icon=./icons/mac/app.icns --ignore='website'",
    "linux": "electron-packager ./ --out=./builds --overwrite --platform=linux --arch=x64 --icon=./icons/png/1024x1024.png --ignore='website'",
    "postpackage": "run-s mac linux",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "format": "prettier --write 'src/**/*.{js,css,md}'",
    "posttest": "npm run format"
  },
  "dependencies": {
    "@brikcss/merge": "^1.3.0",
    "@svgr/core": "^4.3.2",
    "@svgr/plugin-jsx": "^4.3.2",
    "@svgr/plugin-prettier": "^4.3.2",
    "@svgr/plugin-svgo": "^4.3.1",
    "copy-to-clipboard": "^3.2.0",
    "deepmerge": "^4.2.2",
    "eslint-plugin-react": "^7.17.0",
    "lodash.debounce": "^4.0.8",
    "merge-deep": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "overmind": "^21.0.0",
    "overmind-react": "^22.0.0",
    "prettier": "^1.18.2",
    "prism-react-renderer": "^0.1.7",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.8",
    "reakit": "^1.0.0-beta.6",
    "styled-components": "^4.3.2",
    "svgo": "^1.3.0",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "electron": "^9.4.0",
    "electron-packager": "^14.0.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "standard": "^14.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
